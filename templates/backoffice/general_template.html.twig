<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Bonjour!{% endblock %}</title>
    {{ encore_entry_link_tags('backoffice/common/app') }}
    <!-- Latest compiled and minified CSS -->
    {% block stylesheets %}
    {% endblock %}
</head>
<body class="skin-purple sidebar-mini">
{% block body %}
    <div id="wrapper">
        <header class="main-header">
            {% include 'backoffice/layout_partials/top_navbar.html.twig' %}
        </header>
        <aside class="main-sidebar">
            {% include 'backoffice/layout_partials/menu.html.twig' %}
        </aside>
        <div id="content-wrapper">
            <div class="content-wrapper" style="min-height: 685px;">
                {% block wrapper %}
                {% endblock %}
            </div>
        </div>
    </div>
{% endblock %}

{% block common_javascripts %}
    {{ encore_entry_script_tags('backoffice/common/app') }}
    {% block javascripts %} {% endblock %}
{% endblock %}

</body>
</html>
